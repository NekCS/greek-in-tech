{
  "name": "greek-in-tech",
  "version": "0.1.0",
  "description": "Learn which words used on modern computing and software engineering, come straight from the greek language/mythology.",
  "main": "./lib/index.js",
  "engines": {
    "node": ">=0.10"
  },
  "scripts": {
    "clean": "rimraf lib",
    "test": "cross-env BABEL_ENV=commonjs mocha --compilers js:babel-register --recursive",
    "test:watch": "npm test -- --watch",
    "cover": "cross-env BABEL_ENV=commonjs istanbul cover _mocha -- --compilers js:babel-register --recursive",
    "lint": "xo **/*.js !lib/*.js",
    "build": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "prepublish": "npm run clean && npm run lint && npm run test && npm run build",
    "postcodecover": "npm run cover && codecov"
  },
  "files": [
    "lib",
    "data"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/tsevdos/greek-in-tech"
  },
  "keywords": [
    "greek",
    "words",
    "tech",
    "names",
    "list",
    "data",
    "random"
  ],
  "author": "John Tsevdos <tsevdosjohn@gmail.com> (http://tsevdos.me)",
  "license": "GPL3",
  "bugs": {
    "url": "https://github.com/tsevdos/greek-in-tech/issues"
  },
  "homepage": "http://greekintech.com",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.10.1",
    "babel-eslint": "^6.1.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.9.0",
    "chai": "^3.5.0",
    "codecov": "1.0.1",
    "cross-env": "^2.0.0",
    "istanbul": "^1.0.0-alpha",
    "mocha": "^3.0.0",
    "rimraf": "^2.5.2",
    "xo": "^0.16.0"
  },
  "dependencies": {
    "unique-random-array": "^1.0.0"
  },
  "xo": {
    "space": 2,
    "envs": [
      "node",
      "mocha"
    ]
  }
}
